[Unit]
Description=HeytelepatSpeaker
After=network.target

[Service]
Type=notify
WorkingDirectory=/home/pi/heytelepat-speaker

User=pi

ExecStart=/home/pi/heytelepat-speaker/env/bin/python /home/pi/heytelepat-speaker/speaker.py -symd -log=info -infunc=rpibutton
Restart=on-failure

[Install]
WantedBy=multi-user.target
